<table border="1px">
    <thead>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Cedula</th>
        <th>Cumplea√±os</th>
        <th>Genero</th>
        <th>Telefono</th>
        <th>Direccion</th>
        <th>Email</th>
        <th>Estado</th>
    </thead>
    <tbody>
        {{#each client}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.lastName}}</td>
                <td>{{this.documentNumber}}</td>
                <td>{{this.birthdate}}</td>
                <td>{{this.gender}}</td>
                <td>{{this.phone}}</td>
                <td>{{this.address}}</td>
                <td>{{this.email}}</td>
                <td>{{this.state}}</td>
                <td>
                    <form action="/upClient/{{this._id}}" method="get">
                        <button type="submit">Editar</button>
                    </form>
                    <form action="/deleteClient/{{this._id}}" method="post">
                        <button type="submit">Eliminar</button>
                    </form>
                </td>
            </tr>
        {{/each}}
    </tbody>
</table>